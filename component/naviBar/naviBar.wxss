  border-bottom: 0.5rpx solid #eee;
  background-color: white;
  
}
.defoultNaviContent {
  width: 750rpx;
  height: 100%;
  display: flex;
  flex-direction: row;
}
.backItem_img {
  width: 40rpx;
  height: 38rpx;
  margin-left: 22rpx;
}
.leftItem {
  height: 100%;
  width: 187rpx;
  /* background: rgb(1, 1, 1); */
  /* display: flex; */
}
.backItem {
  height: 100%;
  width: 187rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.centerItem {
  width: 376rpx;
  height: 100%;
  display: flex;
}
.titleView {
  
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width: 100%;
  color: black;
  font-size: 36rpx;
  font-weight: bold;
}